    "scripts": {
        "build": "rm -rf dist && npm run build:js && npm run build:css",
        "build:js": "BABEL_ENV=dist babel client -d dist --copy-files",
        "build:css": "postcss \"dist/**/*.css\" --replace",
        "build:frontend": "rm -rf ./scripts/frontend/dist && webpack --config ./scripts/frontend/webpack.config.js",
        "prepublishOnly": "npm run build",
        "start": "node server/app.js",
        "start-dev": "node server/app-dev.js",
        "mocha": "mocha",
        "prettier-client": "prettier --write --tab-width=4 --single-quote --trailing-comma=es5 --jsx-bracket-same-line --no-semi \"client/**/*.js\"",
        "prettier-server": "prettier --write --tab-width=4 --single-quote --trailing-comma=es5 --jsx-bracket-same-line \"server/**/*.js\"",
        "lint": "eslint \"client/**/*.js\"",
        "test": "jest",
        "analyze": "npm run analyze:dev",
        "analyze:dev": "webpack --config webpack.development.config.js --profile --json > stats.json && webpack-bundle-analyzer stats.json ./dist/public",
        "analyze:prod": "webpack --config webpack.production.config.js --profile --json > stats.json && webpack-bundle-analyzer stats.json ./dist/public",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    }
